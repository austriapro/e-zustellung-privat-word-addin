<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.e-zustellung.at/namespaces/ed_remote_control_20150115" 
           xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dsig="http://www.w3.org/2000/09/xmldsig#" 
           targetNamespace="http://www.e-zustellung.at/namespaces/ed_remote_control_20150115" 
           elementFormDefault="qualified" 
           attributeFormDefault="unqualified" 
           id="ed_remote_control_20150115">
	<!-- xs:import namespace="http://www.e-zustellung.at/namespaces/ed_delivery_20150115" schemaLocation="ed_delivery_20150115.xsd"/ -->
	<xs:include schemaLocation="ed_base_20150115.xsd" id="ed_base_20150115"/>
